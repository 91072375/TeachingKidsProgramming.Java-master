package org.teachingkidsprogramming;

public class Print100Ints10by10
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 10; i++)
    {
      System.out.print((i + 1) + " ");
      System.out.print(i);
    }
    for (int i = 0; i < 10; i++)
    {
      System.out.println((i + 1) + " ");
    }
  }
}
