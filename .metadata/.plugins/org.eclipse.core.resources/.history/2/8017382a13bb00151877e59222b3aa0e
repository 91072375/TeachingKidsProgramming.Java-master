package org.teachingkidsprogramming;

public class Print100Ints10by10
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 10; i++)
    {
      System.out.print((i + 1) + " ");
    }
    for (int i = 10; i < 20; i++)
    {
      System.out.print((i + 1) + " ");
    }
    for (int i = 20; i < 30; i++)
    {
      System.out.print((i + 1) + " ");
    }
    for (int i = 30; i < 40; i++)
    {
      System.out.print((i + 1) + " ");
    }
  }
}
