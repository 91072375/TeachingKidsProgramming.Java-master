package org.teachingkidsprogramming;

public class Print100Ints10by10
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 100; i++)
    {
      System.out.print((i + 1) + " ");
      for (int j = 0; j < 100; j++)
      {
        System.out.println((j + 1) + " ");
      }
    }
  }
}